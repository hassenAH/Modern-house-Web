{
  "name": "vue-notus",
  "homepage": "https://demos.creative-tim.com/vue-notus/",
  "version": "1.1.0",
  "description": "Vue Notus - A free Tailwind CSS and VueJS UI Kit and Admin by Creative Tim.",
  "repository": "https://github.com/creativetimofficial/vue-notus",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && gulp licenses",
    "lint": "vue-cli-service lint",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "@vue/compiler-sfc": "3.2.47",
    "axios": "^1.3.4",
    "chart.js": "2.9.4",
    "core-js": "3.9.1",
    "google-auth-library": "^8.7.0",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "1.0.8",
    "sweetalert2": "^11.4.8",
    "tailwindcss": "^2.2.19",
    "three": "^0.152.2",
    "v-file-picker": "^1.1.2",
    "vue": "3.2.0",
    "vue-google-oauth2": "^1.5.10",
    "vue-google-signin": "^2.0.3",
    "vue-image-chooser": "^0.5.14",
    "vue-router": "4.0.5",
    "vue3-google-map": "^0.15.0",
    "vue3-google-oauth2": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@vue-leaflet/vue-leaflet": "^0.9.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "5.0.0-alpha.7",
    "@vue/cli-service": "^5.0.8",
    "autoprefixer": "10.2.5",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.7.0",
    "leaflet": "^1.9.3",
    "postcss": "^8.4.23",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
